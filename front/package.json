{
  "name": "ailumni",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write \"app/**/*.{js,jsx,ts,tsx,css,scss,md,json}\"",
    "check-format": "prettier --check \"app/**/*.{js,jsx,ts,tsx,css,scss,md,json}\"",
    "stylelint": "stylelint \"app/**/*.{css,scss}\"",
    "stylelint:fix": "stylelint \"app/**/*.{css,scss}\" --fix",
    "ci:check": "npm run lint && npm run check-format && npm run stylelint && npm test",
    "style:fix": "npm run lint:fix && npm run format && npm run stylelint:fix"
  },
  "dependencies": {
    "amazon-cognito-identity-js": "^6.0.0",
    "next": "15.3.5",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-toastify": "^11.0.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "eslint": "^9",
    "eslint-config-next": "15.3.5",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.5.1",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.13",
    "stylelint-config-tailwindcss": "^1.0.0",
    "stylelint-order": "^7.0.0",
    "tailwindcss": "^4"
  }
}
